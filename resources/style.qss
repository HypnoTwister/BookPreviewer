}

/* QLineEdit */
QLineEdit#H1{
    border: none;
    font-size: 12px;
    background-color:white;
    border-radius: 4px;
    padding: 4px;
    width: 50px;
    height: 18px;
    margin: 0px;
}

QLineEdit#H2{
    border: none;
    font-size: 12px;
    background-color: #dedede;
    border-radius: 4px;
    padding: 4px;
    width: 50px;
    height:18px;
    margin: 0px;
}

/* QFrame */
QFrame#vline{
    color: darkgray;
}

