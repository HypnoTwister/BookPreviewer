QLabel#HighLight{
    /* font-weight: light; */
    font-size: 12px;
    padding: 0px;
    border: none;
    color: #005fba;
}
/* QLabel */
QLabel#HighLight_Core{
    /* font-weight: light; */
    font-size: 12px;
